{
  "Logging": {
    "LogLevel": { // No provider, LogLevel applies to all the enabled providers.
      "Default": "None",
      "Microsoft": "None",
      "Microsoft.Hosting.Lifetime": "None"
    },
    "Debug": { // Debug provider.
      "LogLevel": {
        "Default": "None" // Overrides preceding LogLevel:Default setting.
      }
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "None",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "None",
        "Microsoft.AspNetCore.Mvc.Razor": "None",
        "Default": "None"
      }
    },
    "EventSource": {
      "LogLevel": {
        "Microsoft": "None"
      }
    },
    "EventLog": {
      "LogLevel": {
        "Microsoft": "None"
      }
    },
    "AzureAppServicesFile": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "None"
      }
    },
    "AzureAppServicesBlob": {
      "IncludeScopes": true,
      "LogLevel": {
        "Microsoft": "None"
      }
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "None"
      }
    }
  },
  "AllowedHosts": "*"
}
